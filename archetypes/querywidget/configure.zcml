<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    xmlns:cmf="http://namespaces.zope.org/cmf"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="plone">

  <include package="plone.app.contentlisting" />
  <include package="plone.app.vocabularies" />

  <include file="skins.zcml" />

  <genericsetup:registerProfile
    name="default"
    title="Querywidget"
    directory="profiles/default"
    description="Archetypes querywidget"
    provides="Products.GenericSetup.interfaces.EXTENSION"
    />

  <browser:page
    name="archetypes.querywidget.querywidget.js"
    for="*"
    permission="zope2.View"
    template="querywidget.js"
    />

  <browser:page
    name="archetypes.querywidget.querywidget.css"
    for="*"
    permission="zope2.View"
    template="querywidget.css"
    />

</configure>
